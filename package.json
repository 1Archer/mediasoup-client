{
  "name": "mediasoup-client",
  "version": "3.2.10",
  "description": "mediasoup client side JavaScript library",
  "author": "Iñaki Baz Castillo <ibc@aliax.net> (https://inakibaz.me)",
  "contributors": [
    "José Luis Millán <jmillan@aliax.net> (https://github.com/jmillan)"
  ],
  "homepage": "https://mediasoup.org",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/versatica/mediasoup-client.git"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "engines": {
    "node": ">=8.6.0"
  },
  "scripts": {
    "typescript:watch": "rm -rf lib; tsc --pretty --watch",
    "typescript:build": "rm -rf lib; tsc && ${NODE} replace-version.js",
    "lint": "MEDIASOUP_NODE_LANGUAGE=typescript eslint -c .eslintrc.js --ext=ts src/ && MEDIASOUP_NODE_LANGUAGE=javascript eslint -c .eslintrc.js --ext=js --ignore-pattern '!.eslintrc.js' .eslintrc.js test/ replace-version.js",
    "lint:fix": "MEDIASOUP_NODE_LANGUAGE=typescript eslint --fix -c .eslintrc.js --ext=ts src/ && MEDIASOUP_NODE_LANGUAGE=javascript eslint --fix -c .eslintrc.js --ext=js --ignore-pattern '!.eslintrc.js' .eslintrc.js test/ replace-version.js",
    "test": "jest",
    "coverage": "jest --coverage && open-cli coverage/lcov-report/index.html"
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "node",
    "testRegex": "test/test.*\\.js"
  },
  "dependencies": {
    "awaitqueue": "^1.0.0",
    "bowser": "^2.7.0",
    "debug": "^4.1.1",
    "events": "^3.0.0",
    "h264-profile-level-id": "^1.0.0",
    "sdp-transform": "^2.13.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/events": "^3.0.0",
    "@types/uuid": "^3.4.5",
    "@typescript-eslint/eslint-plugin": "^2.6.0",
    "@typescript-eslint/parser": "^2.6.0",
    "eslint": "^6.6.0",
    "eslint-plugin-jest": "^22.20.1",
    "jest": "^24.9.0",
    "jest-tobetype": "^1.2.3",
    "node-mediastreamtrack": "0.1.0",
    "open-cli": "^5.0.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4",
    "uuid": "^3.3.3"
  }
}
